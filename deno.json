{
  "name": "@vef/easy-ops",
  "version": "0.1.2",
  "exports": "./mod.ts",
  "publish": {
    "exclude": [
      ".github",
      "dev"
    ]
  },
  "tasks": {
    "dev": "deno run --allow-all --watch dev/main.ts",
    "check": "deno publish --dry-run",
    "update": "deno add @vef/easy-cli && deno add @vef/string-utils"
  },
  "imports": {
    "#/": "./src/",
    "@vef/easy-cli": "jsr:@vef/easy-cli@^0.2.3",
    "@vef/string-utils": "jsr:@vef/string-utils@^0.1.5"
  }
}
